{
    "name": "make-it-app/utils",
    "description": "Just some utils for my packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Kabin",
            "email": "ioffline@make-it.app"
        }
    ],
    "require": {
        "php": "^7.4|^8.1|^8.2|^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MakeIT\\Utils\\": "src/"
        }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "0.x.x-dev"
      },
      "laravel": {
        "aliases": {
          "Sorter": "MakeIT\\Utils\\Sorter"
        }
      }
    },
    "support": {
      "issues": "https://github.com/make-it-app/utils/issues",
      "source": "https://github.com/make-it-app/utils"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
